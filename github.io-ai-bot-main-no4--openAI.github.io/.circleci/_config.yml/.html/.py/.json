            <parameter name="color" type="string" />
        </parameters>
        <body>
            <!-- TensorFlow의 GPU를 사용하도록 설정합니다. -->
            <set_variable name="tf_config" value="tf.ConfigProto(device_count={'GPU': 1})" />
            <set_variable name="session" value="tf.Session(config=tf_config)" />


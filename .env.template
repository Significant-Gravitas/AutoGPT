################################################################################
### AUTO-GPT - GENERAL SETTINGS
################################################################################

## Allow local command execution (Default: False)
# EXECUTE_LOCAL_COMMANDS=False

## Restrict file operations to workspace ./auto_gpt_workspace (Default: True)
# RESTRICT_TO_WORKSPACE=True

## Define the user-agent used by the requests library to browse website (string)
# USER_AGENT="Mozilla/5.0 (Macintosh; Intel Mac OS X 10_15_4) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/83.0.4103.97 Safari/537.36"

## Specifies which AI Settings file to use (defaults to ai_settings.yaml)
# AI_SETTINGS_FILE=ai_settings.yaml

## Specifies which Prompt Settings file to use (defaults to prompt_settings.yaml)
# PROMPT_SETTINGS_FILE=prompt_settings.yaml

## Key to authorise commands
# AUTHORISE_COMMAND_KEY=y

## Key to exit AUTO-GPT
# EXIT_KEY=n

## Enabeling plain output will disable spinner (Default: False)
## Note: Spinner is used to indicate that Auto-GPT is working on something in the background
# PLAIN_OUTPUT=False

## The list of categories of commands that are disabled. Each of the below are an option:
## autogpt.commands.analyze_code
## autogpt.commands.audio_text
## autogpt.commands.execute_code
## autogpt.commands.file_operations
## autogpt.commands.git_operations
## autogpt.commands.google_search
## autogpt.commands.image_gen
## autogpt.commands.improve_code
## autogpt.commands.web_selenium
## autogpt.commands.write_tests
## autogpt.app
## autogpt.commands.task_statuses
# For example, to disable coding related features, uncomment the next line
#
# DISABLED_COMMAND_CATEGORIES=autogpt.commands.analyze_code,autogpt.commands.execute_code,autogpt.commands.git_operations,autogpt.commands.improve_code,autogpt.commands.write_tests

## The list of commands that are NOT ALLOWED to be executed by Auto-GPT (Default: None)
# the following are examples:
# DENY_COMMANDS=cd,nano,vim,vi,emacs,rm,sudo,top,ping,ssh,scp

## ONLY those commands will be allowed to be executed by Auto-GPT (Default: None)
# the following are examples:
# ALLOW_COMMANDS=ls,git,cat,grep,find,echo,ps,curl,wget


################################################################################
### LLM PROVIDER
################################################################################

### OPENAI
## NOTE: https://platform.openai.com/docs/api-reference/completions

## Your OpenAI API Key (Example: my-openai-api-key)
OPENAI_API_KEY=your-openai-api-key

# The temperature setting in language models like GPT controls the balance between
# predictable and random responses. 
# Lower temperature makes the responses more focused and deterministic,
# while higher temperature makes them more creative and varied.
# The temperature range typically goes from 0 to 2 in OpenAI's implementation.
#
## Sets temperature in OpenAI (Default: 0)
# TEMPERATURE=0

## Use Azure OpenAI or not (Default: False)
# USE_AZURE=False

### AZURE
# moved to `azure.yaml.template`

################################################################################
### LLM MODELS
################################################################################

## Smart language model (Default: gpt-4)
## Fast language model (Default: gpt-3.5-turbo)
# SMART_LLM_MODEL=gpt-4
# FAST_LLM_MODEL=gpt-3.5-turbo

### LLM MODEL SETTINGS
## Fast token limit for OpenAI (Default: 4000)
## Smart token limit for OpenAI (Default: 8000)
## When using --gpt3only this needs to be set to 4000.
# FAST_TOKEN_LIMIT=4000
# SMART_TOKEN_LIMIT=8000

### EMBEDDINGS
## Model to use for creating embeddings
# EMBEDDING_MODEL=text-embedding-ada-002

################################################################################
### MEMORY
################################################################################

## Memory backend type : json_file (default), redis (if configured)
# MEMORY_BACKEND=json_file

## Name of index created in Memory backend (Default: auto-gpt-memory)
# MEMORY_INDEX=auto-gpt-memory

### REDIS
## Redis host (Default: localhost, use "redis" for docker-compose)
## Redis port (Default: 6379)
## Redis password (Default: "")
## Wipes data / index on start (Default: True)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=
# WIPE_REDIS_ON_START=True

################################################################################
### IMAGE GENERATION PROVIDER
################################################################################

### COMMON SETTINGS
## Image provider - dalle, huggingface, or sdwebui
## Image size (Example: 256)
## Image sizes for dalle: 256, 512, 1024
# IMAGE_PROVIDER=dalle
# IMAGE_SIZE=256

### HUGGINGFACE
## Text-to-image model from Huggingface (Default: CompVis/stable-diffusion-v1-4)
## HuggingFace API token (Example: my-huggingface-api-token)
# HUGGINGFACE_IMAGE_MODEL=CompVis/stable-diffusion-v1-4
# HUGGINGFACE_API_TOKEN=your-huggingface-api-token

### STABLE DIFFUSION WEBUI
## Stable diffusion webui username:password pair (Example: username:password)
## Stable diffusion webui API URL (Example: http://127.0.0.1:7860)
# SD_WEBUI_AUTH=
# SD_WEBUI_URL=http://127.0.0.1:7860

